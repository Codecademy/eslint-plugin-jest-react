{
  "author": "Codecademy Engineering <dev@codecademy.com>",
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.14.0"
  },
  "description": "ESLint plugin for Jest with React",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "eslint",
    "jest",
    "react",
    "dev"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.yml": [
      "yarn format:prettier"
    ],
    "{cypress,webpack}/**/*.{ts,tsx,js,jsx,json,css,scss}": [
      "yarn format:prettier"
    ]
  },
  "main": "lib/index.js",
  "name": "eslint-plugin-jest-react",
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Codecademy/eslint-plugin-jest-react.git"
  },
  "scripts": {
    "compile": "tsc",
    "format": "yarn prettier --write",
    "format:verify": "yarn prettier --list-different \"**/*.{js,json,md,ts,yml}\"",
    "format:write-all": "yarn format:verify --write",
    "lint": "yarn eslint \"src/**/*.ts\" --max-warnings 0 --report-unused-disable-directives",
    "precommit": "lint-staged",
    "test": "jest",
    "test:ci": "jest --maxWorkers=2"
  },
  "version": "0.0.2"
}
